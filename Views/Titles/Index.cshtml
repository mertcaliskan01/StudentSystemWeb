@model IEnumerable<StudentSystemWeb.Models.Title>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h1>İşsizlik Oranı (%)</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Baslik)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Baslik)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Id">Download</a>
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Id">Ayrıntılı Bilgi Göster</a>
            </td>
        </tr>
}
    </tbody>
</table>
